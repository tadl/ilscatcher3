
    <% if item.copies_available[0].to_i > 0 %>
        <div class="panel panel-primary">
    <% else %>
        <div class="panel panel-default">
    <% end %>
        <div class="panel-heading"><%= item.title %></div>
        <div class="panel-body">
            <img src='https://catalog.tadl.org/opac/extras/ac/jacket/large/r/<%= item.record_id %>' 
                onerror="this.src='<%= asset_path("logo-clock-only.png") %>'" />
        </div><!-- /.panel-body -->
    </div><!-- /.panel -->
