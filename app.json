{
    "name": "ilscatcher3",
    "description": "Traverse Area District Library Online Public Access Catalog System",
    "website": "https://www.tadl.org/",
    "require": {
        "plugins": [
            {
                "name": "redis",
                "commands": ["redis:create redis_$APP", "redis:link redis_$APP $APP"]
            }, {
                "name": "memcached",
                "commands": ["memcached:create memcached_$APP", "memcached:link memcached_$APP $APP"]
            }
        ]
    },
    "scripts": {
        "dokku": {
            "postdeploy": "rake generate_sliders"
        }
    }
}
